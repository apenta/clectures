@*9. Bind this view to IList<Film> *@
@model IList<Forms.Web.Models.Film>

@*3. Our landing page for search results *@
@{
    ViewBag.Title = "SearchResult";
}

<h2>Search Results</h2>

<table>
    @foreach (var film in Model)
    {
        <tr>
            <td>@film.Id</td>
            <td>@film.Title</td>
            <td>@film.Description</td>
            <td>@film.ReleaseYear</td>
            <td>@film.Length</td>
            <td>@film.Rating</td>
        </tr>
    }

</table>